{
  "name": "general-library-starter",
  "author": "Luis Miguel Santamaría Pérez",
  "version": "0.1.1",
  "description": "Libraría base",
  "main": "dist/bundle.js",
  "license": "MIT",
  "babel": {
    "presets": ["flow", "latest"]
  },
  "engines": {
    "node": ">=8.6."
  },
  "release": {
    "analyzeCommits": "semantic-release-tamia/analyzeCommits",
    "generateNotes": "semantic-release-tamia/generateNotes",
    "verifyRelease": "semantic-release-tamia/verifyRelease"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "coveralls": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-flow": "^1.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.52.0",
    "happypack": "^3.0.3",
    "jest": "^20.0.4",
    "semantic-release": "^8.0.3",
    "semantic-release-tamia": "^1.1.1",
    "source-map-loader": "^0.2.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1"
  },
  "scripts": {
    "build": "webpack --config ./webpack.config.js -p",
    "dev": "webpack --config ./webpack.config.js",
    "test": "jest",
    "cover":
      "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "semantic-release":
      "semantic-release pre && npm publish && semantic-release post",
    "changelog:preview": "sr-changelog",
    "changelog:commit": "sr-changelog commit"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/lmsp/general-library-starter.git"
  }
}
